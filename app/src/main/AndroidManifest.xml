<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studymanageroficial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_studymanager"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewTarefa.TarefaDetalhada"></activity>
        <activity android:name=".viewTarefa.CadastroTarefa" />
        <activity android:name=".viewlogin.CadastroLogin" />
        <activity android:name=".viewlogin.LoginView" />
        <activity android:name=".viewDisciplina.DisciplinaDetalhada" />
        <activity android:name=".viewDisciplina.ListDisciplinas" />
        <activity android:name=".viewDisciplina.CadastroDisciplina" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>